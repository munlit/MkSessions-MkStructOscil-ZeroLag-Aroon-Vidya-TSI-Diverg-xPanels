		  column			            = _col,
		  row				            = _row,
		  width				            = _width,
		  bgcolor			            = _bgcolor,
		  text				            = _txt,
		  text_color		            = _txtcolor,
		  text_halign		            = _txthalign,
		  text_size			            = _txtsize
		  )

